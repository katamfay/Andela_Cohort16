def get_primes(n):
    """ this prints a list of primes from 0 to n """
    for x in range(n):
        if n % x ==0:
            return False

    return True

def print_next(n):
    """prints the next prime after the existing number"""
    i = n
    while True:
        i += 1
        if get_primes(i):
            print i
        pass
     



        

        